{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@simple/*": [
        "src/SimpleComponents/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@myTypes/*": [
        "src/types/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    }
  }
}
